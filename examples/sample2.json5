{
    // comments
    mode: 48, // Sv48
    // Sample JSON for running test cases
    memory_size: 0x3000000000, // 192 GB
    lower_bound: 0x2000000000, // Start addresses from at least here
    test_cases: [
        {
            repeats: 4, // How many times to make this kind of test case
            ptes: [
                {},
                {},
                {},
                {
                    attributes: {
                        // Set the flags (RSW, DAGUXWRV) probabilistically on the PTE
                        G: 0.5, // Float for probabilities
                        U: 1, // 0 - 1 aren't probabilities
                        X: 0.5, // Float for probabilities
                        W: 0, // 0 - 1 aren't probabilities
                        R: 1, // 0 - 1 aren't probabilities
                    },
                },
            ],
        },
        {
            repeats: 5,
            reuse_pte: 0.5, // Reuse a previously defined PTE 50% of the time
        },
        {
            reuse_satp: true,
            repeats: 4,
            ptes: [
                {},
                {
                    // PTE entry -- set the address + the PPNs
                    address: 0xcafebabe0,
                },
                {},
            ],
        },
        {
            repeats: 4,
            ptes: [
                {
                    // PTE entry -- set the address + the PPNs
                    // If you use a list wehere a number is needed, it'll choose at random
                    address: [0xdeadbeef0, 0xcafebabe0],
                },
                {},
                {},
            ],
        },
        {
            repeats: 2,
            // Specifying an entire path
            ptes: [
                { address: 0x00001452876690 },
                { address: 0x000011fd18ccb8 },
                { address: 0x000027960d1f80 },
                { address: 0x0000264afd7c28 },
            ],
        },
        {
            "repeats": 2,
            // SATP + VA = exact same path
            "satp.ppn": 0xb00ffff000,
            "va": 0xf00fb00f,
        },
        {
            repeats: 40,
            same_va_pa: 1,
        },
        {
            // Possible to Alias and! do the same VA PA
            repeats: 2,
            same_va_pa: 1,
            aliasing: 1,
        },
        {
            // Same SATP + VA to same page = maps to same physical page
            "satp.ppn": 0xb00ffff000,
            "va": 0xf00ff0000,
        },
        {
            "satp.ppn": 0xb00ffff000,
            "va": 0xf00ff0008,
        },
        {
            repeats: 5,
            pagesize: '512G', // Cases can be set to a different pagesize
        },
        {
            repeats: 5,
            pagesize: ['512G', '1G', '2M'], // Cases can also be set to randomly choose one from a list
        }
    ],
}
